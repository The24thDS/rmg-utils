{
  "name": "rmg-utils",
  "private": true,
  "version": "1.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "cp magick.js public/assets/magick.js && cp magick.wasm public/assets/magick.wasm",
    "build": "tsc && vite build",
    "postbuild": "rm public/assets/magick.js public/assets/magick.wasm",
    "preview": "vite preview",
    "release": "npx standard-version",
    "functions": "npx env-cmd -f ./.env.local netlify functions:serve",
    "e2e": "playwright test",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mantine/core": "^7.0.0",
    "@mantine/dropzone": "^7.0.0",
    "@mantine/form": "^7.0.0",
    "@mantine/hooks": "^7.0.0",
    "@mantine/notifications": "^7.0.0",
    "@netlify/functions": "^1.0.0",
    "@sentry/browser": "^7.12.1",
    "@tabler/icons-react": "^3.5.0",
    "airtable": "^0.11.4",
    "jotai": "^2.4.1",
    "jotai-devtools": "^0.6.2",
    "filesize": "^10.0.12",
    "konva": "^8.3.14",
    "leaflet": "^1.9.4",
    "mantine-contextmenu": "^1.3.15",
    "random": "^3.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-konva": "^18.2.3",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.3.0",
    "tiny-invariant": "^1.2.0",
    "use-debounce": "^9.0.4",
    "use-image": "^1.1.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.3",
    "@playwright/test": "^1.44.1",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.20.0",
