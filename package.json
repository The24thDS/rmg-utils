{
  "name": "rmg-utils",
  "private": true,
  "version": "1.5.2",
  "scripts": {
    "dev": "vite",
    "prebuild": "cp magick.js public/assets/magick.js && cp magick.wasm public/assets/magick.wasm",
    "build": "tsc && vite build",
    "postbuild": "rm public/assets/magick.js public/assets/magick.wasm",
    "preview": "vite preview",
    "release": "npx standard-version",
    "functions": "npx env-cmd -f ./.env.local netlify functions:serve",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.0",
    "@mantine/dropzone": "^6.0.0",
    "@mantine/form": "^6.0.20",
    "@mantine/hooks": "^6.0.0",
    "@mantine/notifications": "^6.0.0",
    "@netlify/functions": "^1.0.0",
    "@sentry/browser": "^7.12.1",
    "@tabler/icons": "^1.78.1",
    "airtable": "^0.11.4",
    "jotai": "^2.4.1",
    "jotai-devtools": "^0.6.2",
    "konva": "^8.3.14",
    "leaflet": "^1.9.4",
    "random": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.3",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.3.0",
    "tiny-invariant": "^1.2.0",
    "use-debounce": "^9.0.4",
    "use-image": "^1.1.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "eslint": "^8.43.0",
    "eslint-config-react-app": "^7.0.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-pwa": "^0.14.4",
    "vitest": "^0.32.0",
    "workbox-build": "^6.5.4",
    "workbox-window": "^6.5.4"
  }
